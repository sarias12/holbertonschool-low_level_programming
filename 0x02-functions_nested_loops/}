#include "holberton.h"

/**
  * print_sign - Entry point
  * function that prints 10 times the alphabet, in lowercase,
  * followed by a new line.
  *
  * @n: - la descirpcion
  *
  * Return: Always 0 (Success)
  */
int _abs(int, n)
{
	if (n > 0)
	{
		n;
	}
	else if (n < 0)
	{
		n *=  -1;
	}
	return (0);
	
}
